<cm:CIMI xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bs="http://creem.siemens.com/schemas/base" xmlns:cim="http://creem.siemens.com/schemas/cim" xmlns:cm="http://creem.siemens.com/schemas/creem" xmlns="http://creem.siemens.com/schemas/creem" state="Alpha" version="0.0.1" lang="en-US">
	<!-- Name the id of the FB by Namespace format Manufacturer.Category.FBName -->
	<!-- ### 方括号 [] 代表编辑器生成项，生成结果不要包含方括号本身, 结果去掉除开头以外的其他注释 -->
	<cm:FunctionBlock id="[Manufacturer.Category.FBName]">
		<cm:Metadata>
			<cm:Namespace>CREEM.FB.{id}</cm:Namespace>
			<cm:NamespaceURI/>
			<!-- 从[Manufacturer.Category.FBName]里面取，用户也可以修改 -->
			<cm:Name>[FBName]</cm:Name>
			<cm:Version>[1.0.0]</cm:Version>
			<cm:Description/>
			<!-- ### Generated by CREEM Editor ### -->
			<!-- ### 不点重新生成按钮的话，UUID每次都一样 -->
			<cm:UUID>[UUID, do not change unless click generate button again]</cm:UUID>
			<cm:Organization>
				<!-- ### 厂商名，默认Siemens AG.，用户可以修改 -->
				<cm:OrganizationName>[Siemens AG.]</cm:OrganizationName>
				<!-- ### Generated by CREEM Editor ### -->
				<cm:DigitalSignature>[SHA-256 of this file]</cm:DigitalSignature>
			</cm:Organization>
			<cm:Certification/>
		</cm:Metadata>
		<cm:UIInformation>
			<cm:Content lang="en-US">
				<cm:DisplayName>[FB Name]</cm:DisplayName>
				<cm:Description>[FB Description, can be empty]</cm:Description>
				<cm:Icon ref="Resource.defaultIcon"/>
			</cm:Content>
			<!-- 中文翻译部分，用户可以不填留空，如果不填那么cm:Content lang="zh-CN"没有子节点 -->
			<cm:Content lang="zh-CN">
				<cm:DisplayName>[功能块中文名]</cm:DisplayName>
				<cm:Description>[功能块中文描述，可留空]</cm:Description>
                		<cm:Input>
					<cm:Port id="[parameterIn1]" displayName="parameterIn1参数中文名"/>
					<cm:Port id="[parameterIn2]" displayName="parameterIn2参数中文名"/>
					<cm:Port id="[parameterIn3]" displayName="parameterIn3参数中文名"/>
				</cm:Input>
				<cm:Output>
					<cm:Port id="[parameterOut1]" displayName="parameterOut1参数中文名"/>
					<cm:Port id="[parameterOut2]" displayName="parameterOut2参数中文名"/>
					<cm:Port id="status" type="状态码"/>
				</cm:Output>
			</cm:Content>
		</cm:UIInformation>
		<cm:Data>
			<cm:Content>
				<cm:Executor location="{id}.py"/>
				<!-- 下拉框选：LogicalNode (默认), Interface, System -->
				<cm:Category type="[LogicalNode]"/>
				<cm:PhysicalNode type="Device">
					<cm:DeviceDescription ref="[TorqueWrench.Atlas.SF400]"/>
				</cm:PhysicalNode>
				<cm:Input>
					<!-- 数量：0到无限多个，可以加个最大值例如10个 -->
					<!-- type read from schema/CREEM_Type.md -->
					<!-- 参数名为驼峰命名，第一步可不做校验 -->
					<!-- UNECE unit read from schema/CREEM.Base.UNECE.xml or schema/UNECE.csv -->
					<!-- 只有UNECE类型，才需要设置unit（必须设），其他参数没unit -->
					<!-- Input里所有简单类型都可以设defaultValue，也可以不设 -->
					<cm:Port id="[parameterIn1]" type="[UNECE]" unit="[mm]" defaultValue="[0]"/>
					<cm:Port id="[parameterIn2]" type="[String]"/>
					<cm:Port id="[parameterIn3]" type="[Number]"/>
				</cm:Input>
				<cm:Output>
					<cm:Port id="[parameterOut1]" type="[String]"/>
					<cm:Port id="[parameterOut2]" type="[String]"/>
					<cm:Port id="status" type="StatusCode"/>
				</cm:Output>
			</cm:Content>
		</cm:Data>
		<cm:Resource>
			<!-- Parse FB base64 of SVG Icon here, otherwise it will show default FB icon instead -->
			<cm:Content id="Resource.defaultIcon" type="Icon" format="SVG">[Base64 of SVG or 3xPNG]</cm:Content>
		</cm:Resource>
	</cm:FunctionBlock>
</cm:CIMI>
